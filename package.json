{
  "name": "data-manager-desktop",
  "productName": "数据管理系统",
  "version": "1.0.0",
  "author": "Gooalgene",
  "description": "App with data validation and many small tools(Gooalgene)",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "repository": {
    "type": "gitlab",
    "url": "http://54.223.70.246:9000/data-management/data-manager-front-end.git"
  },
  "build": {
    "productName": "数据管理系统",
    "appId": "org.datamanager.electron",
    "copyright": "2018@Gooalgene",
    "directories": {
      "output": "build",
      "buildResources": "dist"
    },
    "nsis": {
      "oneClick": true
    },
    "asar": false,
    "files": [
      "dist/electron/**/*",
      "dist/backend/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:all": "node .electron-vue/build.js && electron-builder -mwl",
    "build:win": "node .electron-vue/build.js && electron-builder --win",
    "build:mac": "node .electron-vue/build.js && electron-builder --mac",
    "build:linux": "node .electron-vue/build.js && electron-builder --linux",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "package": "node .electron-vue/package.js",
    "package:darwin": "cross-env BUILD_TARGET=darwin node .electron-vue/package.js",
    "package:linux": "cross-env BUILD_TARGET=linux node .electron-vue/package.js",
    "package:mas": "cross-env BUILD_TARGET=mas node .electron-vue/package.js",
    "package:win": "cross-env BUILD_TARGET=win32 node .electron-vue/package.js",
    "package:clean": "cross-env BUILD_TARGET=clean node .electron-vue/package.js",
    "package:web": "cross-env BUILD_TARGET=web node .electron-vue/package.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "d3": "4.8",
    "element-ui": "^1.4.0",
    "jquery": "^3.2.1",
    "jquery-resizable-dom": "^0.20.0",
    "localforage": "^1.5.0",
    "venn.js": "^0.2.17",
    "vue": "2.3.3",
    "vue-electron": "^1.0.6",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.0.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-component": "^0.9.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.2.0",
    "babili-webpack-plugin": "^0.1.1",
    "cfonts": "^1.1.3",
    "chai": "^4.0.0",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "del": "^2.2.1",
    "devtron": "^1.1.0",
    "electron": "1.6.11",
    "electron-builder": "^19.43.0",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-packager": "^8.5.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.11.1",
    "grunt": "^1.0.1",
    "html-webpack-plugin": "^2.16.1",
    "inject-loader": "^3.0.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "multispinner": "^0.2.1",
    "node-sass": "^4.5.3",
    "require-dir": "^0.3.0",
    "sass-loader": "^6.0.6",
    "spectron": "^3.7.1",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.7",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^12.2.1",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "2.3.3",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  }
}